<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>

    <constant name="struts.objectFactory.spring.autoWire" value="name"/>

    <package name="load" extends="struts-default">
        <action name="loadNotebar" class="loadNotebar">
            <result name="success">note.jsp</result>
        </action>
        <action name="loadnote" class="loadNote">
            <result name="success">notecon.jsp</result>
        </action>
        <action name="noteadd" class="NoteAdd">
            <result name="success">success.html</result>
        </action>
        <action name="loadhome" class="loadHtmlber">
            <result name="success">index.jsp</result>
        </action>
        <action name="loadhtml" class="loadHtml">
            <result name="success">rainbow.jsp</result>
        </action>
        <action name="htmladd" class="HtmlAdd">
            <result name="success">success.html</result>
        </action>
    </package>

</struts>